
public class Main {
	
	
	public static void main(String[] args) {
		int firstNumber = 343;
		int secondNumber = 3432;
		int total = Calculator.add(firstNumber, secondNumber);
		String outputMsg = "The sum of " + firstNumber + " and " + " is " + total;
		System.out.print(outputMsg);
		
		Calculator calc = new Calculator();
		
		total = calc.add(firstNumber, secondNumber);
		outputMsg = "The subraction of " + firstNumber + " and " + " is " + total;
		System.out.print(outputMsg);

	}
	

}
